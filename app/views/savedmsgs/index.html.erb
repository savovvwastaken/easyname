<p id="notice"><%= notice %></p>

<h1>Savedmsgs</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @savedmsgs.each do |savedmsg| %>
      <tr>
        <td><%= savedmsg.text %></td>
        <td><%= link_to 'Show', savedmsg %></td>
        <td><%= link_to 'Edit', edit_savedmsg_path(savedmsg) %></td>
        <td><%= link_to 'Destroy', savedmsg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Savedmsg', new_savedmsg_path %>
